Nhal_bool_t hal_pwr_set_backup_domain(hal_bool_t status)
N{
N    uint16_t count = 0;
N    uint32_t reg = 0;
N    
N    if (status == DISABLE) {
N        /* Disable backup domain / regulator */
N        PWR->CSR &= ~(1u << 9u);        
X        ( (reg_pwr_t *) 0x40007000 )->CSR &= ~(1u << 9u);        
N        return DISABLED;
N    }
N
N    /* Enable backup domain / regulator */
N    PWR->CSR |= (1u << 9u);
X    ( (reg_pwr_t *) 0x40007000 )->CSR |= (1u << 9u);
N
N    /* Wait till regulator is ready and if time out is reached exit */
N    reg = PWR->CSR & (1u << 3u);
X    reg = ( (reg_pwr_t *) 0x40007000 )->CSR & (1u << 3u);
N    while ((reg == 0) && (count != TIME_OUT)) {
X    while ((reg == 0) && (count != 0x1000)) {
N        reg = PWR->CSR & (1u << 3u);
X        reg = ( (reg_pwr_t *) 0x40007000 )->CSR & (1u << 3u);
N        count++;
N    }
N
N    /* Return */
N    if (reg != 0) {
N        return ENABLED;
N    }
N    return DISABLED;
